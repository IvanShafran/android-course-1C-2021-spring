<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.concurrency"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AnrActivity" />
        <activity android:name=".ThreadAndHandlerActivity" />
        <activity android:name=".HandlerAndHandlerActivity" />
        <activity android:name=".AsyncTaskActivity" />
        <activity android:name=".Anr6Activity" />
    </application>

</manifest>